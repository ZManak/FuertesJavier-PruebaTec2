<?xml version="1.0" encoding="UTF-8"?>
<web-app version="4.0" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
    <servlet>
        <servlet-name>turnSv</servlet-name>
        <servlet-class>prueba2.manager.servlet.turnSv</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>personSv</servlet-name>
        <servlet-class>prueba2.manager.servlet.personSv</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>menu</servlet-name>
        <servlet-class>prueba2.manager.servlet.menu</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>turnSv</servlet-name>
        <url-pattern>/turnSv</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>personSv</servlet-name>
        <url-pattern>/personSv</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>menu</servlet-name>
        <url-pattern>/menu</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
